{
  "name": "galaxy-names",
  "version": "0.0.0-semantically-released",
  "description": "Example of simple npm module with tests and ecmascript 6 support.",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel src/index.js -o dist/index.js",
    "postbuild": "cp src/galaxy-names.json dist/galaxy-names.json",
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "mocha test/index.test.js -w --compilers js:babel/register",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec test/index.test.js --compilers js:babel/register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pedroparra/galaxy-names.git"
  },
  "keywords": [
    "galaxy",
    "names"
  ],
  "author": "Pedro J. Parra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pedroparra/galaxy-names/issues"
  },
  "homepage": "https://github.com/pedroparra/galaxy-names#readme",
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.3.0",
    "codecov.io": "^0.1.6",
    "ghooks": "^0.3.2",
    "istanbul": "^0.3.21",
    "mocha": "^2.3.3",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}
